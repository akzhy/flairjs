{
  "name": "@flairjs/client",
  "version": "1.0.0",
  "module": "./dist/esm/index.js",
  "main": "./dist/cjs/index.js",
  "types": "./dist/types/index.d.ts",
  "files": [
    "dist",
    "generated-css",
    "theme.css"
  ],
  "exports": {
    ".": {
      "types": "./dist/types/index.d.ts",
      "import": "./dist/esm/index.js",
      "require": "./dist/cjs/index.js"
    },
    "./theme": "./theme.css",
    "./generated-css/*": {
      "import": "./generated-css/*",
      "require": "./generated-css/*",
      "default": "./generated-css/*"
    }
  },
  "scripts": {
    "build": "rolldown -c",
    "dev": "rolldown -w -c"
  },
  "devDependencies": {
    "@rollup/plugin-typescript": "^12.1.4",
    "@rsbuild/plugin-react": "^1.2.0",
    "csstype": "^3.1.3",
    "rolldown": "1.0.0-beta.37",
    "typescript": "^5.8.3"
  }
}
