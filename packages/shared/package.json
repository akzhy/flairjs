    "dev": "rolldown -w -c",
    "format": "biome format --write"
  },
  "peerDependencies": {
    "@flairjs/client": "workspace:*",
    "@flairjs/core": "workspace:*",
    "esbuild": ">=0.25.0",
    "picomatch": ">=4.0.0"
  },
  "devDependencies": {
    "@biomejs/biome": "catalog:",
    "@flairjs/client": "workspace:*",
    "@flairjs/core": "workspace:*",
    "@rollup/plugin-typescript": "^12.1.4",
    "@types/node": "catalog:",
    "@types/picomatch": "catalog:",
    "rolldown": "catalog:",
    "typescript": "catalog:"
  },
  "dependencies": {
    "esbuild": "catalog:",
    "picomatch": "catalog:"
  }
}